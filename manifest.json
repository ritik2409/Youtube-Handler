{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
     "default_icon": "icons/icon32.png",
      "default_popup": "popup/YoutubeHandler.html",
      "default_title": "A Minimalistic Approach"
   },
   "description": "A extension that helps you to handle youtube from another tabs.",
   "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Youtube Handler",
   "permissions": [ "*://*.youtube.com/*", "tabs" ],
   "version": "1.0"
}
